		"@react-navigation/native-stack": "^7.3.25",
		"@supabase/supabase-js": "^2.55.0",
		"expo": "~53.0.20",
		"expo-camera": "~16.1.11",
		"expo-constants": "~17.1.7",
		"expo-network": "~7.1.5",
